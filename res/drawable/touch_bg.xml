<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    <!-- 默认时的背景图片-->


　　<item android:state_focused="true">   
         <shape>   
             
                <stroke   
                 android:width="1dp"  
                 android:color="@color/white"  
                 android:dashWidth="5dp"  
                 android:dashGap="3dp"/>   
            <!--  圆角 -->   
           	 <solid 
           	     android:color="@color/grey"
           	     />
             <corners   
                 android:radius="5dp"/>  
        </shape>   
    </item>    

　　<!--选中时的图片背景-->

　　<item android:state_selected="true" android:drawable="@color/alp_g33" />
<item android:state_pressed="true"	android:drawable="@color/alp_g33"></item>

　　<!--获得焦点时的图片背景-->

　　<item android:state_focused="true" android:drawable="@color/alp_g33" />
    

</selector>
